    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^0.9.0",
    "@modelcontextprotocol/sdk": "^1.10.1",
    "express": "^5.1.0",
    "zod": "^3.24.3",
    "mongodb": "^6.3.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "mime-types": "^2.1.35",
    "typescript": "^5.8.3",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/mime-types": "^2.1.4"
